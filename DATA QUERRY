SELECT

G.NOME, G.CNPJCPFSEMMASCARA, A.NUMERO, B.NOME, C.NOME, D.NOME, E.NOME

FROM HD_ORDEM A
INNER JOIN HD_CATEGORIAORDEM B              ON B.HANDLE = A.CATEGORIA
INNER JOIN MS_FILIAL C                      ON C.HANDLE = A.FILIAL
INNER JOIN HD_TIPOORDEM D                   ON D.HANDLE = A.TIPO
INNER JOIN SD_SISTEMA E                     ON E.HANDLE = A.SISTEMA
INNER JOIN MS_USUARIO F                     ON F.HANDLE = A.SOLICITANTE
INNER JOIN MS_PESSOA G                      ON G.HANDLE = F.PESSOA

WHERE A.HANDLE = "xxxx"
